<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.milnow5555.restaurantproject">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".View.LoggingView" />
        <activity android:name=".View.ClientMainView" />
        <activity android:name=".View.MyMealsView" />
        <activity android:name=".View.RestaurantInfo" />
        <activity android:name=".View.CreateUserView" />
        <activity android:name=".View.DatabaseLoaderView"></activity>
    </application>

</manifest>